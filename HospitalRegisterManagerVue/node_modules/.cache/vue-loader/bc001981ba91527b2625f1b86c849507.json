{"remainingRequest":"E:\\software\\HospitalManagerVue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!E:\\software\\HospitalManagerVue\\src\\views\\PatientLayout.vue?vue&type=script&lang=js&","dependencies":[{"path":"E:\\software\\HospitalManagerVue\\src\\views\\PatientLayout.vue","mtime":1705024892000},{"path":"E:\\software\\HospitalManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1689001368000},{"path":"E:\\software\\HospitalManagerVue\\node_modules\\babel-loader\\lib\\index.js","mtime":1689001370000},{"path":"E:\\software\\HospitalManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1689001368000},{"path":"E:\\software\\HospitalManagerVue\\node_modules\\vue-loader\\lib\\index.js","mtime":1689001370000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["PatientLayout.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiCA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"PatientLayout.vue","sourceRoot":"src/views","sourcesContent":["<template>\n    <div>\n        <!-- <div class=\"indexImage\">\n        <img src=\"@/assets/hospital.jpeg\" class=\"layoutImage\"/>\n        <span>今天预约挂号总人数：{{orderPeople}}</span>\n      </div> -->\n        <div class=\"indexPeople\">\n            <div class=\"userImage\">\n                <i class=\"iconfont icon-r-user2\" style=\"font-size: 132px\"></i>\n            </div>\n\n            <div class=\"userFont\">\n                <div class=\"spanCure\">\n                    <span>就诊概况</span>\n                </div>\n                <div class=\"spanPeople\">\n                    <span>前方排队人数：{{ orderPeople }}</span>\n                </div>\n            </div>\n        </div>\n        <el-button @click=\"qiandao\" class=\"check\">\n            <i class=\"iconfont icon-qiandao\" style=\"font-size: 132px\"></i><br/>\n            签到\n            </el-button>\n\n        <el-row>\n            <el-col :span=\"24\">\n                <img src=\"@/assets/back.jpg\" style=\"width: 1100px; margin-left: 80px;border-radius: 30px;\">\n            </el-col>\n        </el-row>\n    </div>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\nimport jwtDecode from \"jwt-decode\";\nimport { getToken } from \"@/utils/storage.js\";\nexport default {\n    name: \"PatientLayout\",\n    data() {\n        return {\n            orderPeople: 4,\n            bedPeople: 1,\n            userId: 1,\n        };\n    },\n    methods: {\n        //修改功能：根据患者id返回排队人数\n        requestPeople() {\n            request\n                .get(\"order/orderPeople\",{\n                    params: {\n                        pId: this.userId,\n                    },\n                })\n                .then((res) => {\n                    if (res.data.status !== 200)\n                        return this.$message.error(\"数据请求失败\");\n                   // this.orderPeople = res.data.data;\n                })\n                .catch((err) => {\n                    console.error(err);\n                });\n        },\n        //新增功能：患者签到，根据id进行签到\n        qiandao() {\n            request\n                .get(\"order/qiandao\",{\n                    params:{\n                        pId:this.userId,\n                    }\n                })\n                .then((res) => {                    \n                    if (res.data.status !== 200)\n                        return this.$message.error(\"数据请求失败\");\n                    this.$message('签到成功！');\n                })\n                .catch((err) => {\n                    console.error(err);\n                });\n        },\n         //token解码\n        tokenDecode(token) {\n        if (token !== null) return jwtDecode(token);\n        },\n    },\n    created() {\n        this.userId = this.tokenDecode(getToken()).pId;\n        this.requestPeople();     \n    },\n};\n</script>\n<style lang=\"scss\" scoped>\n.userFont {\n    height: 150px;\n    width: 250px;\n    float: right;\n    color: white;\n    .spanCure {\n        font-size: 15px;\n        margin-top: 60px;\n        margin-bottom: 15px;\n    }\n    .spanPeople {\n        font-size: 18px;\n    }\n}\n\n.userImage {\n    height: 150px;\n    width: 150px;\n    font-size: 130px;\n    color: white;\n    position: relative;\n    left: 40px;\n    top: 10px;\n    float: left;\n}\n.indexPeople {\n    height: 200px;\n    width: 440px;\n    background-color: rgba(184, 211, 224, 0.777);\n    float: left;\n    margin: 30px;\n    margin-left: 150px;\n    border-radius: 30px;\n}\n.indexPeople2{\n    height: 200px;\n    width: 440px;\n\n    margin: 30px;\n}\n.check {\n    background-color:rgba(184, 211, 224, 0.777) ;\n    margin-left: 30px;\n    margin-top: 30px;\n    height:200px;\n    width:440px;\n    font-size: 30px;\n    border-radius: 30px;\n}\n</style>"]}]}