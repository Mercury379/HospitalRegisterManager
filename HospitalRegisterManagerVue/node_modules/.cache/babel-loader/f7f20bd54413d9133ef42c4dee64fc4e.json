EAAA;AAFA,SAAA;AAIA,OAXA,EAYA,KAZA,CAYA,MAAA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAjBA;AAkBA,KA5EA;;AA6EA;AACA,IAAA,QAAA,CAAA,QAAA,EAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,KAAA,IAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,OAAA,CACA,GADA,CACA,gBADA,EACA;AACA,YAAA,MAAA,EAAA;AACA,cAAA,GAAA,EAAA,KAAA,OAAA,CAAA,GADA;AAEA,cAAA,KAAA,EAAA,KAAA,OAAA,CAAA,KAFA;AAGA,cAAA,MAAA,EAAA,KAAA,OAAA,CAAA;AAHA;AADA,WADA,EAQA,IARA,CAQA,GAAA,IAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,OAAA,KAAA,QAAA,CAAA,KAAA,CACA,aADA,CAAA;AAGA,iBAAA,cAAA,GAAA,KAAA;AACA,iBAAA,aAAA;AACA,iBAAA,QAAA,CAAA,OAAA,CAAA,WAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAjBA;AAkBA,SAnBA,MAmBA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OAxBA;AAyBA,KAxGA;;AAyGA;AACA,IAAA,gBAAA,CAAA,IAAA,EAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,aAAA;AACA,KA7GA;;AA8GA;AACA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,UAAA,GAAA,GAAA;AACA,WAAA,aAAA;AACA,KAnHA;;AAoHA;AACA,IAAA,aAAA,GAAA;AACA,MAAA,OAAA,CACA,GADA,CACA,qBADA,EACA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,UAAA,EAAA,KAAA,UADA;AAEA,UAAA,IAAA,EAAA,KAAA,IAFA;AAGA,UAAA,KAAA,EAAA,KAAA;AAHA;AADA,OADA,EAQA,IARA,CAQA,GAAA,IAAA;AACA,aAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA;AACA,aAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AAEA,OAZA;AAaA;;AAnIA,GArCA;;AA0KA,EAAA,OAAA,GAAA;AACA,SAAA,aAAA;AACA;;AA5KA,CAAA","sourcesContent":["<template>\n    <div>\n        <!-- 卡片 -->\n        <el-card style=\"background-color: #fff5ee9f;\">\n\n            <!-- 搜索栏及增加检查 -->\n            <el-row type=\"flex\">\n                <el-col :span=\"6\">\n                    <el-input v-model=\"query\" placeholder=\"请输入名称查询\">\n                        <el-button\n                            slot=\"append\"\n                            style=\"font-size: 18px;\"\n                            @click=\"requestChecks\"\n                        > 搜索</el-button>\n                    </el-input>\n                </el-col>\n                <el-col :span=\"6\"></el-col>\n                <el-col :span=\"6\">\n                    <el-button type=\"info\" class=\"logup\" style=\"font-size: 18px\" @click=\"addFormVisible = true\"> 增加项目</el-button>\n                </el-col>\n            </el-row>\n            <!-- 表格 -->\n            <el-table :data=\"checkData\" stripe style=\"width: 100%\" border>\n                <el-table-column label=\"编号\" prop=\"iId\"></el-table-column>\n                <el-table-column label=\"项目\" prop=\"iName\"></el-table-column>\n                <el-table-column\n                    label=\"价格/元\"\n                    prop=\"iPrice\"\n                ></el-table-column>\n                <el-table-column label=\"类型\" prop=\"iType\"></el-table-column>\n                <el-table-column label=\"操作\" width=\"290\" fixed=\"right\">\n                    <template slot-scope=\"scope\">\n                        <el-button\n                            style=\"font-size: 18px\"\n                            type=\"success\"\n                            @click=\"modifyDialog(scope.row.iId)\"\n                        > 编辑</el-button>\n                        <el-button\n                            style=\"font-size: 18px\"\n                            type=\"danger\"\n                            @click=\"deleteDialog(scope.row.iId)\"\n                        > 删除</el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n\n            <!-- 分页 -->\n            <el-pagination\n                @size-change=\"handleSizeChange\"\n                @current-change=\"handleCurrentChange\"\n                background\n                layout=\"total, sizes, prev, pager, next, jumper\"\n                :current-page=\"pageNumber\"\n                :page-size=\"size\"\n                :page-sizes=\"[1, 2, 4, 8, 16]\"\n                :total=\"total\"\n            >\n            </el-pagination>\n        </el-card>\n\n        <!-- 增加检查项目对话框 -->\n        <el-dialog title=\"增加检查项目\" :visible.sync=\"addFormVisible\">\n            <el-form :model=\"addForm\" :rules=\"rules\" ref=\"ruleForm\">\n                <el-form-item label=\"编号\" prop=\"iId\" label-width=\"80px\">\n                    <el-input v-model.number=\"addForm.iId\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"名称\" prop=\"iName\" label-width=\"80px\">\n                    <el-input v-model=\"addForm.iName\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"价格\" prop=\"iPrice\" label-width=\"80px\">\n                    <el-input v-model=\"addForm.iPrice\"></el-input>\n                </el-form-item>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"addFormVisible = false\" style=\"font-size: 18px;\"> 取 消</el-button>\n                <el-button type=\"primary\" @click=\"addCheck('ruleForm')\"\n                    style=\"font-size: 18px;\"> 确 定</el-button\n                >\n            </div>\n        </el-dialog>\n\n        <!-- 修改检查项目药物对话框 -->\n        <el-dialog title=\"修改检查项目\" :visible.sync=\"modifyFormVisible\">\n            <el-form :model=\"modifyForm\" :rules=\"rules\" ref=\"ruleForm\">\n                <el-form-item label=\"编号\" prop=\"iId\" label-width=\"80px\">\n                    <el-input\n                        v-model.number=\"modifyForm.iId\"\n                        disabled\n                    ></el-input>\n                </el-form-item>\n                <el-form-item label=\"名称\" prop=\"iName\" label-width=\"80px\">\n                    <el-input v-model=\"modifyForm.iName\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"价格\" prop=\"iPrice\" label-width=\"80px\">\n                    <el-input v-model=\"modifyForm.iPrice\"></el-input>\n                </el-form-item>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"modifyFormVisible = false\" style=\"font-size: 18px;\"> 取 消</el-button>\n                <el-button type=\"primary\" @click=\"modifyCheck('ruleForm')\"\n                    style=\"font-size: 18px;\"> 确 定</el-button\n                >\n            </div>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\n\nexport default {\n    name: \"CheckList\",\n    data() {\n        return {\n            pageNumber: 1,\n            size: 8,\n            query: \"\",\n            checkData: [],\n            total: 3,\n            addFormVisible: false,\n            addForm: {},\n            rules: {\n                //iId: [\n                //    { required: true, message: \"请输入编号\", trigger: \"blur\" },\n                //    {\n                 //       type: \"number\",\n                 //       message: \"账号必须数字类型\",\n                  //      trigger: \"blur\",\n                 //   },\n                //],\n                iName: [\n                    { required: true, message: \"请输入名称\", trigger: \"blur\" },\n                    {\n                        min: 1,\n                        max: 50,\n                        message: \"账号必须是1到50个字符\",\n                        trigger: \"blur\",\n                    },\n                ],\n                iPrice: [\n                    { required: true, message: \"请输入单价\", trigger: \"blur\" },\n                ],\n            },\n            modifyFormVisible: false,\n            modifyForm: {},\n        };\n    },\n    methods: {\n        //点击修改药物信息\n        modifyCheck(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    request\n                        .get(\"check/modifyCheck\", {\n                            params: {\n                                iId: this.modifyForm.iId,\n                                iName: this.modifyForm.iName,\n                                iPrice: this.modifyForm.iPrice,\n                            },\n                        })\n                        .then((res) => {\n                            if (res.data.status !== 200)\n                                return this.$message.error(\"修改信息失败！\");\n                            this.modifyFormVisible = false;\n                            this.requestChecks();\n                            this.$message.success(\"修改检查项目信息成功！\");\n                            console.log(res);\n                        });\n                } else {\n                    console.log(\"error submit!!\");\n                    return false;\n                }\n            });\n        },\n        //打开修改对话框\n        modifyDialog(id) {\n            request\n                .get(\"check/findCheck\", {\n                    params: {\n                        iId: id,\n                    },\n                })\n                .then((res) => {\n                    if (res.data.status !== 200)\n                        return this.$message.error(\"请求数据失败\");\n                    this.modifyForm = res.data.data;\n                    this.modifyFormVisible = true;\n                    console.log(res);\n                });\n        },\n        //删除检查操作\n        deleteCheck(id) {\n            request\n                .get(\"check/deleteCheck\", {\n                    params: {\n                        iId: id,\n                    },\n                })\n                .then((res) => {\n                    this.requestChecks();\n                    console.log(res);\n                });\n        },\n        //删除对话框\n        deleteDialog(id) {\n            this.$confirm(\"此操作将删除该检查项目信息, 是否继续?\", \"提示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    this.deleteCheck(id);\n                    this.$message({\n                        type: \"success\",\n                        message: \"删除成功!\",\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消删除\",\n                    });\n                });\n        },\n        //点击增加确认按钮\n        addCheck(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    request\n                        .get(\"check/addCheck\", {\n                            params: {\n                                iId: this.addForm.iId,\n                                iName: this.addForm.iName,\n                                iPrice: this.addForm.iPrice,\n                            },\n                        })\n                        .then((res) => {\n                            if (res.data.status !== 200)\n                                return this.$message.error(\n                                    \"编号不合法或已被占用！\"\n                                );\n                            this.addFormVisible = false;\n                            this.requestChecks();\n                            this.$message.success(\"增加检查项目成功！\");\n                            console.log(res);\n                        });\n                } else {\n                    console.log(\"error submit!!\");\n                    return false;\n                }\n            });\n        },\n        //页面大小改变时触发\n        handleSizeChange(size) {\n            this.size = size;\n            this.requestChecks();\n        },\n        //   页码改变时触发\n        handleCurrentChange(num) {\n            console.log(num);\n            this.pageNumber = num;\n            this.requestChecks();\n        },\n        // 加载检查列表\n        requestChecks() {\n            request\n                .get(\"check/findAllChecks\", {\n                    params: {\n                        pageNumber: this.pageNumber,\n                        size: this.size,\n                        query: this.query,\n                    },\n                })\n                .then((res) => {\n                    this.checkData = res.data.data.checks;\n                    this.total = res.data.data.total;\n                    \n                });\n        },\n    },\n    created() {\n        this.requestChecks();\n    },\n};\n</script>\n<style scoped lang=\"scss\">\n.el-table {\n    margin-top: 20px;\n    margin-bottom: 20px;\n}\n.el-form {\n    margin-top: 0;\n}\n</style>"],"sourceRoot":"src/views"}]}